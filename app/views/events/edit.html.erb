<%= form_with(model: @event) do |f| %>
    
    <div class="title">
        <%= f.label :title %>
        <%= f.text_field :title %>
    </div>
    
    <div class="time">
        <%= f.label :start_time %>
        <%= f.datetime_select :start_time %>
    </div>
    
    <div class="content">
        <%= f.label :contnet %>
        <%= f.text_field :content %>
    </div>
    
    <div class ="submit">
        <%= f.submit %>
    </div>

<% end %>

<% link_to 'トップページに戻る', root_url %>